{
  "name": "simple-form-demo",
  "version": "0.3.0",
  "main": "index.js",
  "author": "Szilard Hajba <szilu@symbion.hu>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist/assets dist/assets-dev",
    "lint": "tslint -p tsconfig.json",
    "ts": "tsc --noEmit -p tsconfig.json",
    "ts-w": "tsc --noEmit --watch -p tsconfig.json",
    "build": "npm run ts && BABEL_ENV=client parcel build --no-source-maps --detailed-report --experimental-scope-hoisting --public-url /assets --out-dir dist/assets main.tsx",
    "build-w": "BABEL_ENV=client parcel watch --no-autoinstall --public-url /form-simple/assets-dev -d dist/assets-dev main.tsx"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/helper-builder-binary-assignment-operator-visitor": "^7.10.4",
    "@babel/node": "^7.10.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-compress": "^2.0.2",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.3",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "@symbion/simple-form": "^0.4.0",
    "bootstrap": "^4.5.2",
    "fp-ts": "^2.8.2",
    "io-ts": "^2.2.10",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
