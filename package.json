{
  "name": "simple-form-demo",
  "version": "0.1.0",
  "main": "index.js",
  "author": "Szilard Hajba <szilu@symbion.hu>",
  "license": "ISC",
  "scripts": {
    "ts": "tsc --noEmit -p tsconfig.json",
    "ts-w": "tsc --noEmit --watch -p tsconfig.json",
    "build": "npm run ts && BABEL_ENV=client parcel build --no-source-maps --detailed-report --experimental-scope-hoisting --public-url /assets --out-dir dist/assets main.tsx",
    "build-w": "BABEL_ENV=client parcel watch --no-autoinstall --public-url /form-simple/assets-dev -d dist/assets-dev main.tsx"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/helper-builder-binary-assignment-operator-visitor": "^7.8.3",
    "@babel/node": "^7.8.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.9.4",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-compress": "^2.0.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.6",
    "bootstrap": "^4.4.1",
    "fp-ts": "^2.5.4",
    "io-ts": "^2.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
